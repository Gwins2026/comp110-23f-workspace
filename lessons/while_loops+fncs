def mimic(my_words: str) -> str:
    """Given the strings my_words, outputs the same string"""
    return my_words



def mimic_letter(my_words: str, letter idx: int) -> str:
    """Outputs the character of my_words at index letter_idx"""
    if letter_idx >= len(my_words) 
        return("Too high of an index")
    else:
        return my_word[letter_idx]